{
  "name": "sprout",
  "type": "module",
  "version": "0.1.1",
  "description": "Simple seeded fake data generator for TypeScript",
  "license": "MIT",
  "keywords": ["fake", "data", "generator", "seed"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:js": "bun build src/index.ts --outdir dist --target bun --external typescript --minify",
    "build:dts": "tsc --noEmit false --emitDeclarationOnly --declaration",
    "build": "bun run build:js && bun run build:dts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@cstrlcs/configs": "latest",
    "@types/bun": "latest",
    "eslint": "latest",
    "typescript": "latest"
  }
}
